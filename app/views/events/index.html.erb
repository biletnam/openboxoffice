<table>
  <tr>
    <th>Name</th>
    <th>Description</th>
    <th>Date</th>
    <th>Location</th>
  </tr>

<% @events.each do |event| %>
  <tr>
    <td><%= link_to event.name, edit_location_event_path(event.location, event) %></td>
    <td><%= event.description %></td>
    <td><i>Coming soon...</i></td>
    <td><%= event.location.name%></td>
  </tr>
<% end %>
</table>

<br />
